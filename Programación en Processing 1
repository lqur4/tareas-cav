void setup() {
  size(600, 600);
}
void draw() {
    
  background(249, 231, 159);
  fill(44,100,100);//dar color
  ellipse(150, 150, 70, 70);
  ellipse(450, 150, 70, 70);
  
    textSize(32); // establece el tamaño del texto
  text("¡Hola, mundo!", 350, 550);  // pone el texto en la posición (x, y)
  
  translate(width/1.9,height/90);//traslada las figuras a la posición que se desee
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/40);//gira la figura

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/40);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/40);
  
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/40);

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/35);

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/35);

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/35);
  
   ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/40);

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/40);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/40);
  
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/40);

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/35);

  ellipse(300, 300, 50, 50);
  fill(120,300,40);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(44,300,200);
  rotate(PI/35);
  
  ellipse(300, 300, 50, 50);
  fill(252, 243, 207);
  rotate(PI/35);

  ellipse(300, 300, 50, 50);
